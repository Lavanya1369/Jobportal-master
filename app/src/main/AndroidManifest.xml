<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.l9ench.jobportal">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Secondclass"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Login"
            android:label="@string/title_activity_login"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.l9ench.jobportal.MainActivity" />
        </activity>
        <activity
            android:name=".ThirdActivity"
            android:label="@string/title_activity_third"
            android:parentActivityName=".Login"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FourthActivity"
            android:label="@string/title_activity_fourth"
            android:parentActivityName=".ThirdActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.l9ench.jobportal.ThirdActivity" />
        </activity>
        <activity
            android:name=".FifthActivity"
            android:label="@string/title_activity_fifth"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SixthActivity" />
        <activity
            android:name=".TestActivity"
            android:theme="@style/VerticalIntroStyle">

        </activity>
        <activity android:name=".SevenActivity" />



        <activity
            android:name=".Splash"
            android:label="@string/title_activity_splash"
            android:theme="@style/AppTheme.NoActionBar">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

</manifest>